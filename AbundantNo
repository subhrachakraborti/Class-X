import java.util.*;
public class AbundantNo
{
    public static void main()
    {
        Scanner in = new Scanner (System.in);
        System.out.println("ENTER A NO.");
        int n = in.nextInt();
        int sum=0;
        for(int i=1; i<n; i++)
        {
            if(n%i == 0)
            sum += i;
        }
        if(sum > n)
        System.out.println("Abundant No.");
        else
        System.out.println("Not Abundant No.");
    }
}
