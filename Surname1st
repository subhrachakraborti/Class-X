import java.util.*;
class Surname1st
{
    public static void main()
    {
        Scanner in = new Scanner (System.in);
        System.out.println("Enter Your Name!");
        String st = in.nextLine();
        int p = st.length();
        char ch; int x=0, n=1;
        for(int i=0; i<p; i++)
        {
            ch = st.charAt(i);
            x = st.lastIndexOf(" ");
        }
        System.out.println(st.substring(x+1,p)+" "+st.substring(0,x));
    }
}
