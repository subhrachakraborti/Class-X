public class Pattern103
{
    public static void main()
    {
        for (int i = 1; i <= 5; i++) 
        { 
            for (int j = 1; j < i; j++)
                System.out.print(i + " ");
            for (int k = i; k <= 5; k++)
                System.out.print(k + " ");
            System.out.println();
        }
    }
}
