import java.util.*;
class Array3
{
    public static void main()
    {
        Scanner in = new Scanner (System.in);
        int i;
        int m[]=new int[2];
        for(i=0;i<2;i++)
        {
            System.out.print("Enter Your Inputs!");
            m[i]=in.next().charAt(0);
            m[i]=in.nextInt();
        }
        for(i=0;i<2;i++)
        {
            System.out.println(m[i]);
        }
    }
}
