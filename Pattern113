public class Pattern113
{
    public static void main()
    {
        System.out.println("A");
        for(int i=66; i<=69; i++)
        {
            for(int j=0; j<2; j++)
            System.out.print((char)i);
            System.out.println();
        }
        for(int i=70; i>=66; i--)
        {
            for(int j=0; j<2; j++)
            System.out.print((char)i);
            System.out.println();
        }
        System.out.println("A");
    }
}
